<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gwangrok.gallery">


    <uses-sdk
        android:minSdkVersion="5"
        android:targetSdkVersion="21"/>

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:name="com.example.gwangrok.gallery.ParseApplication"
        android:icon="@drawable/photosharing"
        android:label="PhotoSharing"
        android:theme="@style/AppTheme"


        >
        <activity
            android:name="com.example.gwangrok.gallery.MainActivity"
            android:label="PhotoSharing"

            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.gwangrok.gallery.LoginSignupActivity" >
        </activity>
        <activity android:name="com.example.gwangrok.gallery.Welcome" >
        </activity>
        <activity android:name="com.example.gwangrok.gallery.ReceiveActivity" >
        </activity>
        <activity android:name="com.example.gwangrok.gallery.MyPicActivity" >
        </activity>
        <activity android:name="com.example.gwangrok.gallery.CameraActivity" >
        </activity>
        <activity android:name="com.example.gwangrok.gallery.SendActivity" >

        </activity>
    </application>

</manifest>